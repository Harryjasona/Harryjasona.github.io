{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/deldersveld/topojson/master/world-countries.json",
    "format": {
          "type": "topojson",
          "feature": "countries1"}
  },
  "title": {
      "text": "End of 2022 Global Intrest Rates",
      "subtitle": "A Global Persepctive on Intrest Rates, Source: Trading Economics, deldersveld ",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },

   "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Harryjasona/Harryjasona.github.io/main/Project/intrest_rate_bu_country.csv"
        },
        "key": "Country",
        "fields": ["Last"]
      }
    }
  ],
  "projection": {
    "type": "naturalEarth1"},
    
  "config": {
  "background":"white",
  "view": {"stroke": "transparent"}},

  "mark": {
    "type": "geoshape",
    "stroke": "black",
    "strokeWidth":0.1},
  
  "encoding": {"color": {
      "field": "Last",
      "type": "quantitative"
    }, "tooltip": [{"field": "properties.name", "title": "Country"},{"field": "Last",
        "title": "Intrest Rate Dec 2022"}]}
}
