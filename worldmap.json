{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/Harryjasona/Harryjasona.github.io/main/WorldMap.geo.json",
    "title": {
      "text": "The Base Map",
      "subtitle": " United Kingdom By Local District Authorities, Source: Github - martinjc",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "format": {"property": "features"}
  },
  "mark": {
    "type": "geoshape",
    "fill": "rgb(50,50,50)",
    "stroke": "white",
    "strokeWidth": 0.5
  },
  "encoding": {"tooltip": {"field": "properties.name_en"}}
}
